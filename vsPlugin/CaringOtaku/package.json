{
    "name": "caring-otaku",
    "displayName": "CaringOtaku",
    "description": "CaringOtaku",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "icon": "https://i.ibb.co/3p9N0Vp/icon.png",
    "contributes": {
        "commands": [{
            "command": "extension.caringOtaku",
            "title": "Caring Otaku"
        }],
        "keybindings": [{
            "command": "extension.caringOtaku",
            "key": "ctrl+shift+,",
            "mac": "ctrl+shift+,"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}